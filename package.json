{
  "name": "UNotes",
  "version": "0.1.7",
  "publisher": "ryanmcalister",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:unoteFiles"
  ],
  "main": "./ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unotes.start",
        "title": "Unotes"
      },
      {
        "command": "unotes.addNote",
        "title": "Add Note",
        "icon": {
          "light": "resources/light/document.svg",
          "dark": "resources/dark/document.svg"
        }
      },
      {
        "command": "unotes.addFolder",
        "title": "Add Folder",
        "icon": {
          "light": "resources/light/folder.svg",
          "dark": "resources/dark/folder.svg"
        }
      },
      {
        "command": "unotes.addNoteHere",
        "title": "Add Note",
        "icon": {
          "light": "resources/light/document.svg",
          "dark": "resources/dark/document.svg"
        }
      },
      {
        "command": "unotes.addFolderHere",
        "title": "Add Folder",
        "icon": {
          "light": "resources/light/folder.svg",
          "dark": "resources/dark/folder.svg"
        }
      },
      {
        "command": "unotes.deleteFolder",
        "title": "Remove Folder",
        "icon": {
          "light": "resources/light/folder.svg",
          "dark": "resources/dark/folder.svg"
        }
      },
      {
        "command": "unotes.deleteNote",
        "title": "Remove Note",
        "icon": {
          "light": "resources/light/document.svg",
          "dark": "resources/dark/document.svg"
        }
      },
      {
        "command": "unotes.moveUp",
        "title": "Move Up",
        "icon": {
          "light": "resources/light/arrow_up.svg",
          "dark": "resources/dark/arrow_up.svg"
        }
      },
      {
        "command": "unotes.moveDown",
        "title": "Move Down",
        "icon": {
          "light": "resources/light/arrow_down.svg",
          "dark": "resources/dark/arrow_down.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "unotes.addNote",
          "when": "view == unoteFiles",
          "group": "navigation"
        },
        {
          "command": "unotes.addFolder",
          "when": "view == unoteFiles",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "unotes.addNoteHere",
          "when": "view == unoteFiles",
          "group": "unotesAdd@1"
        },
        {
          "command": "unotes.addFolderHere",
          "when": "view == unoteFiles",
          "group": "unotesAdd@2"
        },
        {
          "command": "unotes.deleteNote",
          "when": "viewItem == uNoteFile"
        },
        {
          "command": "unotes.deleteFolder",
          "when": "viewItem == uNoteFolder"
        },
        {
          "command": "unotes.moveDown",
          "when": "viewItem == uNoteFile",
          "group": "inline@2"
        },
        {
          "command": "unotes.moveUp",
          "when": "viewItem == uNoteFile",
          "group": "inline@1"
        }
      ]
    },
    "keybindings": [
      {
        "command": "unotes.heading.1",
        "key": "ctrl+alt+1",
        "mac": "cmd+alt+1"
      },
      {
        "command": "unotes.heading.2",
        "key": "ctrl+alt+2",
        "mac": "cmd+alt+2"
      },
      {
        "command": "unotes.heading.3",
        "key": "ctrl+alt+3",
        "mac": "cmd+alt+3"
      },
      {
        "command": "unotes.heading.4",
        "key": "ctrl+alt+4",
        "mac": "cmd+alt+4"
      },
      {
        "command": "unotes.heading.5",
        "key": "ctrl+alt+5",
        "mac": "cmd+alt+5"
      },
      {
        "command": "unotes.heading.6",
        "key": "ctrl+alt+6",
        "mac": "cmd+alt+6"
      },
      {
        "command": "unotes.normal",
        "key": "ctrl+alt+0",
        "mac": "cmd+alt+0"
      },
      {
        "command": "unotes.bold",
        "key": "ctrl+alt+b",
        "mac": "cmd+alt+b"
      },
      {
        "command": "unotes.italic",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i"
      },
      {
        "command": "unotes.strike",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      },
      {
        "command": "unotes.task",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t"
      },
      {
        "command": "unotes.ul",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u"
      },
      {
        "command": "unotes.ol",
        "key": "ctrl+alt+o",
        "mac": "cmd+alt+o"
      },
      {
        "command": "unotes.blockquote",
        "key": "ctrl+alt+q",
        "mac": "cmd+alt+q"
      },
      {
        "command": "unotes.code",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h"
      },
      {
        "command": "unotes.codeblock",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      },
      {
        "command": "unotes.indent",
        "key": "",
        "mac": ""
      },
      {
        "command": "unotes.outdent",
        "key": "",
        "mac": ""
      },
      {
        "command": "unotes.hr",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "unotes",
          "title": "UNotes",
          "icon": "unotes.svg"
        }
      ]
    },
    "views": {
      "unotes": [
        {
          "id": "unoteFiles",
          "name": "Note Files"
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "debounce": "^1.2.0",
    "fast-glob": "^2.2.6"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "typescript": "^3.2.4",
    "vscode": "^1.1.25"
  },
  "scripts": {
    "vscode:prepublish": "rmdir /s /q build && cd editor && node build-non-split.js && move build .. && cd ..",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "rmdir /s /q build && cd editor && node build-non-split.js && move build .. && cd .."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
