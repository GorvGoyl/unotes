{
  "name": "UNotes",
  "version": "0.1.1",
  "publisher": "ryanmcalister",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:noteFiles"
  ],
  "main": "./ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unotes.start",
        "title": "Unotes"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "unotes",
          "title": "UNotes",
          "icon": "unotes.svg"
        }
      ]
    },
    "views": {
      "unotes": [
        {
          "id": "noteFiles",
          "name": "Note Files"
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "fast-glob": "^2.2.6"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "typescript": "^3.2.4",
    "vscode": "^1.1.25"
  },
  "scripts": {
    "vscode:prepublish": "rmdir /s /q build && cd editor && node build-non-split.js && move build .. && cd ..",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
