{
  "name": "UNotes",
  "version": "0.1.2",
  "publisher": "ryanmcalister",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:noteFiles"
  ],
  "main": "./ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unotes.start",
        "title": "Unotes"
      },
      {
        "command": "unotes.heading1",
        "title": "Unotes Heading 1"
      }
    ],
    "keybindings": [
      {
        "command": "unotes.heading.1",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1"
      },
      {
        "command": "unotes.heading.2",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2"
      },
      {
        "command": "unotes.heading.3",
        "key": "ctrl+shift+3",
        "mac": "cmd+shift+3"
      },
      {
        "command": "unotes.heading.4",
        "key": "ctrl+shift+4",
        "mac": "cmd+shift+4"
      },
      {
        "command": "unotes.heading.5",
        "key": "ctrl+shift+5",
        "mac": "cmd+shift+5"
      },
      {
        "command": "unotes.heading.6",
        "key": "ctrl+shift+6",
        "mac": "cmd+shift+6"
      },
      {
        "command": "unotes.normal",
        "key": "ctrl+shift+0",
        "mac": "cmd+shift+0"
      },
      {
        "command": "unotes.bold",
        "key": "ctrl+alt+b",
        "mac": "cmd+alt+b"
      },
      {
        "command": "unotes.italic",
        "key": "ctrl+i",
        "mac": "cmd+i"
      },
      {
        "command": "unotes.strike",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      },
      {
        "command": "unotes.task",
        "key": "ctrl+t",
        "mac": "cmd+t"
      },
      {
        "command": "unotes.ul",
        "key": "ctrl+u",
        "mac": "cmd+u"
      },
      {
        "command": "unotes.ol",
        "key": "ctrl+o",
        "mac": "cmd+o"
      },
      {
        "command": "unotes.blockquote",
        "key": "ctrl+alt+q",
        "mac": "cmd+alt+q"
      },
      {
        "command": "unotes.code",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h"
      },
      {
        "command": "unotes.codeblock",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      },
      {
        "command": "unotes.indent",
        "key": "",
        "mac": ""
      },
      {
        "command": "unotes.outdent",
        "key": "",
        "mac": ""
      },
      {
        "command": "unotes.hr",
        "key": "ctrl+l",
        "mac": "cmd+l"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "unotes",
          "title": "UNotes",
          "icon": "unotes.svg"
        }
      ]
    },
    "views": {
      "unotes": [
        {
          "id": "noteFiles",
          "name": "Note Files"
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "fast-glob": "^2.2.6"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "typescript": "^3.2.4",
    "vscode": "^1.1.25"
  },
  "scripts": {
    "vscode:prepublish": "rmdir /s /q build && cd editor && node build-non-split.js && move build .. && cd ..",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "rmdir /s /q build && cd editor && node build-non-split.js && move build .. && cd .."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
